<Application x:Class="UIFlatDesign_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UIFlatDesign_WPF"
             xmlns:viewModal="clr-namespace:UIFlatDesign_WPF.MVMM.ViewModal"
             xmlns:view="clr-namespace:UIFlatDesign_WPF.MVMM.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewModal:HomeViewModal}">
                <view:HomeView/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type viewModal:ProjectsViewModal}">
                <view:ProjectsView/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type viewModal:EducationViewModal}">
                <view:EducationView/>
            </DataTemplate>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
